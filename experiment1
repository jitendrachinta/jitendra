      "cell_type": "code",
      "source": [
        "import pandas as pd\n",
        "import numpy as np\n",
        "#Load the dataset\n",
        "dataset=pd.read_csv(\"/content/age_salary_dataset (2).csv\")\n",
        "#printing the dataset\n",
        "dataset\n",
        "X=dataset.iloc[:,:-1].values\n",
        "Y=dataset.iloc[:,-1].values\n",
        "print('independent variable(X):',X)\n",
        "print('dependent variable(Y):',Y)\n",
        "from sklearn.impute import SimpleImputer\n",
        "imp=SimpleImputer(missing_values=np.nan,strategy=\"mean\")\n",
        "X=imp.fit_transform(X)\n",
        "Y=Y.reshape(-1,1)\n",
        "Y=imp.fit_transform(Y)\n",
        "Y=Y.reshape(-1)\n",
        "print(X)\n",
        "print(Y)\n",
        "X_df=pd.DataFrame(X,columns=dataset.columns[:-1])\n",
        "Y_df=pd.DataFrame(Y,columns=[dataset.columns[-1]])\n",
        "combined_data=pd.concat([X_df,Y_df],axis=1)\n",
        "print(\"Combined Data:\")\n",
        "print(combined_data)\n",
        "combined_df=pd.DataFrame(combined_data,columns=[\"column1\",\"column2\", ...])\n",
        "combined_df.to_excel(\"/content/combined_data.xlsx\")\n",
        "print(\"combined DataFrame:\")\n",
        "print(combined_df.head())"
      ],
